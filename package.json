{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.0"
  },
  "private": true,
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "watch-css": "npm run watch-css --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run -p watch-css\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^1.2.0",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "fuzzball": "^0.11.6",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.4.3",
    "validator": "^8.1.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  }
}
