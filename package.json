{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.0"
  },
  "private": true,
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "watch-css": "npm run watch-css --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run -p watch-css\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "async": "^2.5.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "compromise": "^10.7.2",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^1.2.0",
    "csv-stringify": "^1.0.4",
    "csvtojson": "^1.1.7",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "fast-csv": "^2.4.0",
    "fuzzball": "^0.11.6",
    "js-levenshtein": "^1.1.2",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^4.10.8",
    "morgan": "^1.8.2",
    "natural": "^0.5.4",
    "node-fetch": "^1.7.3",
    "node-wordnet": "^0.1.12",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "^2.4.3",
    "validator": "^8.1.0",
    "wndb-with-exceptions": "^3.0.2",
    "wordnet-db": "^3.1.6",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.11.0"
  }
}
